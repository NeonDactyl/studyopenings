{
  "name": "studyopenings",
  "version": "1.0.0",
  "description": "An opening a day takes the blunders away!",
  "scripts": {
    "compile": "tsc",
    "copystatic": "cp -r src/client/css src/client/img src/client/*.html src/client/*.ico build/client",
    "copyclientlib": "cp -r src/client/js/lib build/client/js",
    "start": "npm run compile && npm run copystatic && npm run copyclientlib && npm run webpack && node build/server/main.js",
    "start-dev": "npm run compile && npm run copystatic && npm run copyclientlib && npm run webpack-dev && node build/server/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "npx webpack",
    "webpack-dev": "npx webpack --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jven/studyopenings.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jven/studyopenings/issues"
  },
  "homepage": "https://github.com/jven/studyopenings#readme",
  "dependencies": {
    "@types/auth0-js": "^8.11.7",
    "@types/express": "^4.16.0",
    "@types/mongodb": "^3.1.17",
    "@types/toastr": "^2.1.35",
    "auth0-js": "^9.8.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.15.2",
    "express-jwt": "^5.3.1",
    "express-jwt-authz": "^1.0.0",
    "http": "0.0.0",
    "jwks-rsa": "^1.3.0",
    "mongodb": "^3.1.10"
  },
  "engines": {
    "node": "10.3.0"
  },
  "devDependencies": {
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
